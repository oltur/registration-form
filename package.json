    "ignore-styles": "^5.0.1",
    "prop-types": "^15.5.9",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.5.2",
    "redux-reset": "^0.3.0",
    "redux-saga": "^0.16.0",
    "sinon": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.0.4",
